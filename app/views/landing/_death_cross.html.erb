<div class="wrapper wrapper-content animated fadeInRight">

<div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5> Death Cross </h5>
                <div class="ibox-tools">
                    <a class="collapse-link">
                        <i class="fa fa-chevron-up"></i>
                    </a>
                </div>
            </div>
            <div class="ibox-content">

                <table class="table table-hover">
                    <thead>
                    <tr>
                        <th> Company Name </th>
                        <th> Symbol </th>
                        <th> Date </th>
                        <th> Stock Price </th>
                        <th> Price Changes</th>
                    </tr>
                    </thead>
                    <tbody id='death_cross_list'>
                         <%= render 'death' %>
                    </tbody>
                </table>
                <div class="death_cross">
                  <%= will_paginate @death_cross_stocks %>
                </div>
                
            </div>
        </div>
    </div>
</div>

